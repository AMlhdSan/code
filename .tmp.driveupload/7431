#include <bits/stdc++.h>

#define N 10001

using namespace std;

int d, p, c, f;

int head[N], nxt[N], to[N], w[N];
int cnt = 0;

void add(int u, int v, int c) {
    nxt[++cnt] = head[u];
    head[u] = v;
    w[cnt] = c;
    to[cnt] = v;
}

void spfa() {

}

int main() {

    cin >> d >> p >> c >> f;

    for(int i = 1; i <= p; ++i) {
        int u, v;
        cin >> u >> v;
    }

    return 0;
}